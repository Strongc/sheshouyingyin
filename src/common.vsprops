<?xml version="1.0" encoding="windows-1250"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="common"
	OutputDirectory="$(ConfigurationName)"
	IntermediateDirectory="$(OutDir)"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(SolutionDir)include\;$(SolutionDir)src\base"
		PreprocessorDefinitions="WINVER=0x0601;_WIN32_WINNT=0x0601;NTDDI_VERSION=NTDDI_WIN7;PSAPI_VERSION=1"
		EnableFunctionLevelLinking="true"
		WarningLevel="3"
		DisableSpecificWarnings="4995;4996"
	/>
	<Tool
		Name="VCLibrarianTool"
		AdditionalLibraryDirectories="$(SolutionDir)\lib\"
	/>
</VisualStudioPropertySheet>
